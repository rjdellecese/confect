{
  "name": "confect-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently --restart-tries=-1 --prefix-colors='white.bold.bgCyan,white.bold.bgYellow' --prefix=' {name} ' --names='VITE,CONVEX' 'vite' 'convex dev --tail-logs'",
    "pnpm:devPreinstall": "[ -d node_modules ] && rm -r node_modules; cd .. && pnpm build",
    "postinstall": "pnpm convex codegen",
    "typecheck:vite": "tsc --noEmit --project tsconfig.json",
    "typecheck:convex": "tsc --noEmit --project convex/tsconfig.json",
    "build": "vite build --mode development"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@effect/platform": "0.90.0",
    "@rjdellecese/confect": "file:../",
    "convex": "1.25.4",
    "effect": "3.17.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "vite": "7.0.6"
  },
  "devDependencies": {
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "4.7.0",
    "concurrently": "9.2.0",
    "typescript": "5.9.2"
  }
}
