{
  "name": "example",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently --restart-tries=1 --prefix-colors='white.bold.bgCyan,white.bold.bgYellow' --prefix=' {name} ' --names='VITE,CONVEX' 'vite' 'convex dev --tail-logs'",
    "convex-codegen": "pnpm convex codegen",
    "typecheck": "tsc --noEmit --project tsconfig.json && tsc --noEmit --project convex/tsconfig.json",
    "build": "vite build --mode development",
    "lint": "prettier --check . && eslint . --max-warnings=0",
    "fix": "prettier --write . && eslint --fix . --max-warnings=0",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@effect/platform": "0.90.0",
    "@rjdellecese/confect": "workspace:*",
    "convex": "1.25.4",
    "effect": "3.17.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "vite": "7.1.1"
  },
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@types/node": "24.2.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "5.0.0",
    "concurrently": "9.2.0",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.0"
  }
}
