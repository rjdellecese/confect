{
  "name": "@confect/cli",
  "version": "0.0.1",
  "description": "Confect CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjdellecese/confect.git"
  },
  "bugs": {
    "url": "https://github.com/rjdellecese/confect/issues"
  },
  "homepage": "https://confect.rjdellecese.com",
  "sideEffects": false,
  "type": "module",
  "bin": {
    "confect": "./dist/index.mjs"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist",
    "package.json",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "fix": "prettier --write . && eslint --fix . --max-warnings=0",
    "lint": "prettier --check . && eslint . --max-warnings=0",
    "clean": "rm -rf dist coverage node_modules"
  },
  "keywords": [
    "effect",
    "convex",
    "cli"
  ],
  "author": "RJ Dellecese",
  "license": "ISC",
  "dependencies": {
    "@effect/cli": "^0.72.1",
    "code-block-writer": "^13.0.3",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@effect/language-service": "0.56.0",
    "@effect/platform-node": "0.100.0",
    "@effect/vitest": "0.27.0",
    "@eslint/js": "9.39.1",
    "@tsconfig/strictest": "2.0.8",
    "@types/node": "24.10.1",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.39.1",
    "prettier": "3.6.2",
    "tsdown": "0.16.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.4",
    "vite": "7.2.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@confect/server": "workspace:*",
    "@effect/platform": "^0.93.2",
    "effect": "^3.19.3"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
