{
	"name": "@rjdellecese/confect",
	"version": "0.0.1",
	"description": "Use Effect with Convex!",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"scripts": {
		"start": "convex dev",
		"lint": "biome ci --error-on-warnings",
		"fix": "biome check --write",
		"test": "vitest --run",
		"typecheck": "vitest run --typecheck",
		"coverage": "vitest run --coverage",
		"build": "tsup src/index.ts --format cjs,esm --dts",
		"release": "pnpm build && changeset publish"
	},
	"keywords": ["effect", "convex"],
	"author": "RJ Dellecese",
	"license": "ISC",
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@changesets/cli": "2.27.8",
		"@effect/platform": "0.63.0",
		"@effect/platform-node": "0.58.0",
		"@effect/vitest": "0.9.0",
		"@swc/jest": "0.2.36",
		"@tsconfig/strictest": "2.0.5",
		"@vitest/coverage-v8": "2.0.5",
		"convex-test": "0.0.29",
		"dotenv": "16.4.5",
		"tsup": "8.2.4",
		"typescript": "5.5.4",
		"vite": "5.4.2",
		"vite-tsconfig-paths": "5.0.1",
		"vitest": "2.0.5"
	},
	"dependencies": {
		"@effect/schema": "0.72.0",
		"convex": "1.15.0",
		"effect": "3.7.0"
	},
	"engines": {
		"node": ">=20",
		"pnpm": ">=9"
	}
}
