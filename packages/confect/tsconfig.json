{
  "compilerOptions": {
    "plugins": [
      {
        "name": "@effect/language-service"
      }
    ],
    "noEmit": true,
    "importsNotUsedAsValues": "remove",
    "exactOptionalPropertyTypes": false,
    "lib": ["ESNext", "DOM"],
    "target": "ESNext",
    "moduleResolution": "Bundler",
    "module": "ESNext"
  },
  "exclude": ["coverage", "dist", "node_modules", "test/convex"],
  "extends": ["@tsconfig/strictest/tsconfig"]
}
