{
  "name": "@confect/server",
  "version": "1.0.0-next.2",
  "description": "Backend bindings to the Convex platform",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjdellecese/confect.git"
  },
  "bugs": {
    "url": "https://github.com/rjdellecese/confect/issues"
  },
  "homepage": "https://confect.dev",
  "sideEffects": false,
  "type": "module",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist",
    "package.json",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json",
    "./internal/*": null
  },
  "scripts": {
    "build": "tsdown --config-loader unrun",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "fix": "prettier --write . && eslint --fix . --max-warnings=0",
    "format": "prettier --check .",
    "lint": "eslint . --max-warnings=0",
    "clean": "rm -rf dist coverage node_modules"
  },
  "keywords": [
    "effect",
    "convex"
  ],
  "author": "RJ Dellecese",
  "license": "ISC",
  "devDependencies": {
    "@confect/cli": "workspace:*",
    "@confect/test": "workspace:*",
    "@effect/cluster": "0.56.3",
    "@effect/experimental": "0.58.0",
    "@effect/language-service": "0.73.1",
    "@effect/platform": "0.94.4",
    "@effect/platform-node": "0.104.1",
    "@effect/rpc": "0.73.0",
    "@effect/sql": "0.49.0",
    "@effect/vitest": "0.27.0",
    "@effect/workflow": "0.16.0",
    "@eslint/js": "9.39.2",
    "@swc/jest": "0.2.39",
    "@types/node": "25.2.3",
    "@vitest/coverage-v8": "3.2.4",
    "convex-test": "0.0.41",
    "dotenv": "17.3.1",
    "effect": "3.19.16",
    "eslint": "9.39.2",
    "prettier": "3.8.1",
    "tsdown": "0.20.3",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0",
    "vite": "7.3.1",
    "vite-tsconfig-paths": "6.1.1",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@confect/core": "workspace:*",
    "@effect/platform": "^0.94.4",
    "convex": "^1.30.0",
    "effect": "^3.19.16"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
