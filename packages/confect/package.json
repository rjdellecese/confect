{
  "name": "@rjdellecese/confect",
  "version": "0.0.28",
  "description": "Use Effect with Convex!",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjdellecese/confect.git"
  },
  "bugs": {
    "url": "https://github.com/rjdellecese/confect/issues"
  },
  "homepage": "https://rj-dellecese.gitbook.io/confect",
  "sideEffects": false,
  "type": "module",
  "bin": {
    "confect": "./dist/cli/index.js"
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server/index.js"
      },
      "require": {
        "types": "./dist/server/index.d.cts",
        "default": "./dist/server/index.cjs"
      }
    },
    "./api": {
      "import": {
        "types": "./dist/api/index.d.ts",
        "default": "./dist/api/index.js"
      },
      "require": {
        "types": "./dist/api/index.d.cts",
        "default": "./dist/api/index.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server/index.js"
      },
      "require": {
        "types": "./dist/server/index.d.cts",
        "default": "./dist/server/index.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react/index.d.ts",
        "default": "./dist/react/index.js"
      },
      "require": {
        "types": "./dist/react/index.d.cts",
        "default": "./dist/react/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "check-bundled-types": "pnpm build && attw --pack --profile=node16",
    "convex-codegen": "pnpm convex codegen",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "fix": "prettier --write . && eslint --fix . --max-warnings=0",
    "lint": "prettier --check . && eslint . --max-warnings=0",
    "test": "vitest --run --typecheck --coverage",
    "test:watch": "vitest --typecheck --coverage",
    "clean": "rm -rf dist coverage node_modules"
  },
  "keywords": [
    "effect",
    "convex"
  ],
  "author": "RJ Dellecese",
  "license": "ISC",
  "dependencies": {
    "@effect/cli": "^0.72.1",
    "code-block-writer": "^13.0.3",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@effect/cluster": "0.52.9",
    "@effect/experimental": "0.57.1",
    "@effect/language-service": "0.56.0",
    "@effect/platform-node": "0.100.0",
    "@effect/rpc": "0.72.1",
    "@effect/sql": "0.48.0",
    "@effect/vitest": "0.27.0",
    "@effect/workflow": "^0.12.5",
    "@eslint/js": "9.39.1",
    "@swc/jest": "0.2.39",
    "@tsconfig/strictest": "2.0.8",
    "@types/node": "24.10.1",
    "@vitest/coverage-v8": "3.2.4",
    "convex-test": "0.0.38",
    "dotenv": "17.2.3",
    "eslint": "9.39.1",
    "prettier": "3.6.2",
    "tsdown": "0.16.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.4",
    "vite": "7.2.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@effect/platform": "^0.93.2",
    "convex": "^1.29.1",
    "effect": "^3.19.3"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  }
}
