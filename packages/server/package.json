{
  "name": "@confect/server",
  "version": "0.0.1",
  "description": "Confect server",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjdellecese/confect.git"
  },
  "bugs": {
    "url": "https://github.com/rjdellecese/confect/issues"
  },
  "homepage": "https://confect.rjdellecese.com",
  "sideEffects": false,
  "type": "module",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist",
    "package.json",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json",
    "./internal/*": null
  },
  "scripts": {
    "build": "tsdown --config-loader unrun",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "fix": "prettier --write . && eslint --fix . --max-warnings=0",
    "lint": "prettier --check . && eslint . --max-warnings=0",
    "clean": "rm -rf dist coverage node_modules"
  },
  "keywords": [
    "effect",
    "convex"
  ],
  "author": "RJ Dellecese",
  "license": "ISC",
  "devDependencies": {
    "@confect/cli": "workspace:*",
    "@confect/test": "workspace:*",
    "@effect/cluster": "0.52.9",
    "@effect/experimental": "0.57.1",
    "@effect/language-service": "0.56.0",
    "@effect/platform-node": "0.100.0",
    "@effect/rpc": "0.72.1",
    "@effect/sql": "0.48.0",
    "@effect/vitest": "0.27.0",
    "@effect/workflow": "^0.12.5",
    "@eslint/js": "9.39.1",
    "@swc/jest": "0.2.39",
    "@tsconfig/strictest": "2.0.8",
    "@types/node": "24.10.1",
    "@vitest/coverage-v8": "3.2.4",
    "convex-test": "0.0.38",
    "dotenv": "17.2.3",
    "eslint": "9.39.1",
    "prettier": "3.6.2",
    "tsdown": "0.16.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.4",
    "vite": "7.2.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@confect/core": "workspace:*",
    "@effect/platform": "^0.93.2",
    "convex": "^1.30.0",
    "effect": "^3.19.3"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
