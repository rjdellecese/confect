{
	"name": "@rjdellecese/confect",
	"version": "0.0.5",
	"description": "Use Effect with Convex!",
	"repository": {
		"type": "git",
		"url": "https://github.com/rjdellecese/confect.git"
	},
	"bugs": {
		"url": "https://github.com/rjdellecese/confect/issues"
	},
	"homepage": "https://rj-dellecese.gitbook.io/confect",
	"sideEffects": false,
	"type": "module",
	"files": [
		"src",
		"dist",
		"README.md",
		"LICENSE"
	],
	"exports": {
		"./data-model": {
			"import": {
				"types": "./dist/data-model/index.d.ts",
				"import": "./dist/data-model/index.js"
			},
			"require": {
				"types": "./dist/data-model/index.d.cts",
				"require": "./dist/data-model/index.cjs"
			}
		},
		"./ctx": {
			"import": {
				"types": "./dist/ctx/index.d.ts",
				"import": "./dist/ctx/index.js"
			},
			"require": {
				"types": "./dist/ctx/index.d.cts",
				"require": "./dist/ctx/index.cjs"
			}
		},
		"./functions": {
			"import": {
				"types": "./dist/functions/index.d.ts",
				"import": "./dist/functions/index.js"
			},
			"require": {
				"types": "./dist/functions/index.d.cts",
				"require": "./dist/functions/index.cjs"
			}
		},
		"./schema": {
			"import": {
				"types": "./dist/schema/index.d.ts",
				"import": "./dist/schema/index.js"
			},
			"require": {
				"types": "./dist/schema/index.d.cts",
				"require": "./dist/schema/index.cjs"
			}
		},
		"./schemas": {
			"import": {
				"types": "./dist/schemas/index.d.ts",
				"import": "./dist/schemas/index.js"
			},
			"require": {
				"types": "./dist/schemas/index.d.cts",
				"require": "./dist/schemas/index.cjs"
			}
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"start": "convex dev",
		"lint": "biome ci --error-on-warnings",
		"fix": "biome check --write",
		"test": "vitest --run",
		"typecheck": "vitest run --typecheck",
		"coverage": "vitest run --coverage",
		"build": "tsup",
		"release": "pnpm build && changeset publish"
	},
	"keywords": [
		"effect",
		"convex"
	],
	"author": "RJ Dellecese",
	"license": "ISC",
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@changesets/cli": "^2.27.8",
		"@effect/platform": "^0.64.0",
		"@effect/platform-node": "^0.59.0",
		"@effect/vitest": "^0.9.2",
		"@swc/jest": "^0.2.36",
		"@tsconfig/strictest": "^2.0.5",
		"@vitest/coverage-v8": "^2.0.5",
		"convex-test": "^0.0.29",
		"dotenv": "^16.4.5",
		"tsup": "^8.2.4",
		"typescript": "^5.6.2",
		"vite": "^5.4.3",
		"vite-tsconfig-paths": "^5.0.1",
		"vitest": "^2.0.5"
	},
	"dependencies": {
		"@effect/schema": "^0.72.3",
		"convex": "^1.15.0",
		"effect": "^3.7.2"
	},
	"engines": {
		"node": ">=20",
		"pnpm": ">=9"
	}
}
