{
  "name": "@confect/cli",
  "version": "1.0.0-next.1",
  "description": "Developer tooling for codegen and sync",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjdellecese/confect.git"
  },
  "bugs": {
    "url": "https://github.com/rjdellecese/confect/issues"
  },
  "homepage": "https://confect.dev",
  "sideEffects": false,
  "type": "module",
  "bin": {
    "confect": "./dist/index.mjs"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist",
    "package.json",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown --config-loader unrun",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "fix": "prettier --write . && eslint --fix . --max-warnings=0",
    "format": "prettier --check .",
    "lint": "eslint . --max-warnings=0",
    "clean": "rm -rf dist coverage node_modules"
  },
  "keywords": [
    "effect",
    "convex",
    "cli"
  ],
  "author": "RJ Dellecese",
  "license": "ISC",
  "dependencies": {
    "@effect/cli": "^0.73.2",
    "@effect/platform-node": "0.104.1",
    "@effect/platform-node-shared": "0.57.1",
    "@effect/printer": "^0.47.0",
    "@effect/printer-ansi": "^0.47.0",
    "code-block-writer": "^13.0.3",
    "esbuild": "^0.27.3",
    "tsx": "^4.21.0"
  },
  "devDependencies": {
    "@effect/language-service": "0.73.1",
    "@effect/platform": "0.94.4",
    "@effect/vitest": "0.27.0",
    "@eslint/js": "9.39.2",
    "@types/node": "25.2.3",
    "@vitest/coverage-v8": "3.2.4",
    "effect": "3.19.16",
    "eslint": "9.39.2",
    "prettier": "3.8.1",
    "tsdown": "0.20.3",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0",
    "vite": "7.3.1",
    "vite-tsconfig-paths": "6.1.1",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@confect/core": "workspace:*",
    "@confect/server": "workspace:*",
    "@effect/platform": "^0.94.4",
    "effect": "^3.19.16"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
