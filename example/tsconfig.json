{
	"compilerOptions": {
		"exactOptionalPropertyTypes": false,
		"lib": ["ESNext", "DOM"],
		"target": "ESNext",
		"moduleResolution": "Bundler",
		"module": "ESNext",
		"baseUrl": ".",
		"jsx": "react-jsx",
		"paths": {
			"~/*": ["*"]
		}
	},
	"exclude": ["node_modules"],
	"extends": ["@tsconfig/strictest/tsconfig"]
}
