{
  "name": "example",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently --restart-tries=1 --prefix-colors='white.bold.bgCyan,white.bold.bgYellow' --prefix=' {name} ' --names='VITE,CONVEX' 'vite' 'convex dev --tail-logs'",
    "convex-codegen": "pnpm convex codegen",
    "typecheck": "tsc --noEmit --project tsconfig.json && tsc --noEmit --project convex/tsconfig.json",
    "build": "vite build --mode development",
    "format": "prettier --check .",
    "lint": "eslint . --max-warnings=0",
    "fix": "prettier --write . && eslint --fix . --max-warnings=0",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@effect/platform": "0.94.4",
    "@confect/cli": "workspace:*",
    "@confect/core": "workspace:*",
    "@confect/server": "workspace:*",
    "@confect/react": "workspace:*",
    "convex": "1.31.7",
    "effect": "3.19.16",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "vite": "7.3.1"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "25.2.3",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "concurrently": "9.2.1",
    "eslint": "9.39.2",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0"
  }
}
